{
	"private": true,
	"name": "@utilize/json-schema-to-typescript",
	"version": "15.0.3",
	"description": "Core pieces of json-schema-to-typescript",
	"author": "Boris Cherny <boris@borischerny.com> (https://borischerny.com)",
	"repository": {
		"type": "git",
		"url": "git@github.com:MatejBransky/utilize-json-schema.git",
		"directory": "packages/json-schema-to-typescript"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/bcherny/json-schema-to-typescript/issues"
	},
	"homepage": "https://github.com/bcherny/json-schema-to-typescript#readme",
	"keywords": [
		"json",
		"schema",
		"typescript",
		"compile",
		"transpile",
		"api",
		"interface",
		"typing",
		"share"
	],
	"type": "module",
	"main": "./dist/index.mjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		}
	},
	"scripts": {
		"build": "bunchee",
		"check:format": "prettier --check ./{src,test}/**/*.ts",
		"check:types": "tsc --noEmit",
		"format": "pnpm check:format --write",
		"check:lint": "eslint .",
		"test": "vitest",
		"ci:test": "vitest run"
	},
	"dependencies": {
		"@apidevtools/json-schema-ref-parser": "^11.5.5",
		"@types/json-schema": "^7.0.15",
		"@types/lodash": "^4.17.7",
		"lodash": "^4.17.21"
	},
	"devDependencies": {
		"@total-typescript/tsconfig": "^1.0.4",
		"@types/node": "^20.12.7",
		"@utilize/eslint-config": "^3.0.0",
		"@utilize/prettier-config": "^1.1.3",
		"bunchee": "^6.5.0",
		"eslint": "^9.22.0",
		"prettier": "^3.5.3",
		"typescript": "^5.4.5",
		"vitest": "^3.0.9"
	},
	"prettier": "@utilize/prettier-config"
}
